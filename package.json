{
  "name": "@tooltwist/vue-contentservice",
  "version": "0.2.11",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --open ./src/testapp/main.js",
    "build": "vue-cli-service build",
    "test": "vue-cli-service build",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --formats commonjs --name vue-contentservice ./src/index.js",
    "e2e:open": "vue-cli-service e2e:open",
    "patch-release": "npm version patch && npm publish --registry=https://registry.npmjs.org/ --access public && git push --follow-tags",
    "p1": "npm version patch",
    "p2": "npm publish --access public",
    "p3": "git push --follow-tags",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "inspect": "vue-cli-service inspect ./src/index.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/runtime-corejs2": "^7.4.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.4",
    "axios": "^1.12.0",
    "babel-loader": "^8.0.4",
    "bulma": "^0.7.1",
    "caniuse-lite": "^1.0.30000974",
    "debounce": "^1.1.0",
    "file-saver": "^1.3.8",
    "i": "^0.3.6",
    "jwt-decode": "^2.2.0",
    "npm": "^6.5.0",
    "query-string": "^6.0.0",
    "v-clipboard": "^2.0.1",
    "v-hotkey": "^0.2.0",
    "vue": "^3.0.0",
    "vue-drag-drop": "^1.1.2",
    "vue-froala-wysiwyg": "^2.8.4",
    "vue-split-panel": "^1.0.4",
    "vuex": "^3.0.1",
    "webpack": "^4.20.2"
  },
  "peerDependencies": {
    "vue-split-panel": "^1.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-e2e-cypress": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.4",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-preset-vue-app": "^2.0.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.7.1",
    "hoek": ">=4.2.1",
    "lodash.camelcase": "^4.3.0",
    "merge-options": ">=1.0.1",
    "node-sass": "^7.0.0",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "randomatic": ">=3.0.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tooltwist/vue-contentservice.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "author": "Philip Callender <philip.callender@tooltwist.com",
  "description": "VueJS plugin for contentservice.io",
  "license": "MIT",
  "main": "./dist/vue-contentservice.common.js",
  "keywords": [
    "vue",
    "tooltwist",
    "contentservice"
  ]
}
